{
  "extends": "airbnb-base",
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "plugins": ["node"],
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-dynamic-require": 0,
    "import/newline-after-import": 0,
    "newline-per-chained-call": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "prefer-promise-reject-errors": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "no-throw-literal": 0,
    "no-useless-escape": 0,
    "no-unused-expressions": 0,
    "radix": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-unused-vars": [2, {
      "args": "none"
    }],
    "no-multi-assign": 0,
    "no-param-reassign": 0,
    "no-nested-ternary": 0,
    "camelcase": 0,
    "no-mixed-operators": 0,
    "array-callback-return": 0,
    "node/no-unsupported-features": 2
  }
}